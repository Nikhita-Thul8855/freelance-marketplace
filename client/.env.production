# Production Environment Variables for Frontend
# Add these to your Vercel/Netlify environment variables

# API Configuration (Update with your deployed backend URL)
REACT_APP_API_URL=https://your-backend-app.onrender.com/api
# Alternative for Railway: https://your-app.up.railway.app/api

# App Configuration
REACT_APP_APP_NAME=Freelance Marketplace
REACT_APP_VERSION=1.0.0

# Build Configuration
GENERATE_SOURCEMAP=false
CI=false

# Stripe Configuration (Production Keys)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_publishable_key_here

# Optional: Analytics and Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PWA=false
